# Google Generative AI API Key
GOOGLE_AI_API_KEY=your_api_key_here

# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:3000

# AI Configuration
# Timeout for AI move generation in milliseconds (default: 30000 = 30 seconds)
AI_TIMEOUT_MS=30000

# Maximum number of retries if AI fails (default: 3)
AI_MAX_RETRIES=3

# Delay between retries in milliseconds (default: 2000 = 2 seconds)
AI_RETRY_DELAY_MS=2000